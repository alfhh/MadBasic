program fibo;

methods:

	int fibo2(int n){
		vars:
			int acum2;
			int temp1, temp2;

		if(n == 0){
			print("at zero");
			acum2 = 0;
		} else {
			if(n == 1){
				acum2 = 1;
				print("at one");
			} else {
				acum2 = fibo2(n - 1) + fibo2(n - 2);
				print("acum -> " ^ acum2 ^ " n: " ^ n);
			}
		}
		return acum2;
	}


main:
	vars:
	    int a, b, c,d, x;
	
	c = 15;
	b = fibo2(c);
	print(b);
	print("-----");

end
